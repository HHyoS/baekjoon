#include <cstdio>
#include <algorithm>
#include <cmath>
using namespace std;
#define M 1000000
int arr[M] = { 0, };
bool p[10] = { 0, };
int main() {
    int n,m;
    scanf("%d%d", &n,&m);
    if (m > 0) {
        int a;
        while (m--) {
            scanf("%d", &a);
            p[a] = 1;
        }
    }
    for (int a = 0; a <= M; ++a) {
        if (a <= 9 && p[a] == 0) {
            arr[a] = 1;
        }
        else{
            int temp = a;
            int count = 0;
            while (temp > 0) {
                if (p[temp % 10] == 0) {
                    temp /= 10;
                    ++count;
                }
                else
                    break;
            }
            if (temp == 0)
                arr[a] = count;
        }
    }
    arr[100] = 0;
    if (n == 100) {
        printf("0");
        return 0;
    }
    int answer = abs(n - 100);
    for (int a = n; a <= M; ++a) {
        if ((arr[a] > 0)&&((arr[a] + abs(a - n)) < answer)) {
            answer = arr[a] + abs(a - n);
            break;
        }
    }
    for (int a = n; a >= 0; --a) {
        if ((arr[a] > 0) && ((arr[a] + abs(n - a)) < answer)) {
            answer = arr[a] + abs(n - a);
            break;
        }
    }

    printf("%d", answer);
    return 0;
}
