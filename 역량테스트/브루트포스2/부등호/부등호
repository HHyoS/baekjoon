#include <cstdio>
#include <cmath>
#include <cstring>
using namespace std;


int k;
int ar[10] = { 0, };
char arr[10] = { "", };
bool check[10] = { 0, };
int min = -1;
int max = -1;
void dfs(int index) {
    if (k == index) {
        for(int a = 0; a<= index; ++a)
            printf("%d", ar[a]);
        printf("\n");
        return;
    }
    for (int a = 0; a < 10; ++a) {
        if (!check[a]) {
            if ((arr[index] == '>') && (ar[index] > a)) {
                check[a] = 1;
                ar[index+1] = a;
                dfs(index + 1);
                check[a] = 0;
            }
            else if((arr[index] == '<') && (ar[index] < a)) {
                check[a] = 1;
                ar[index+1] = a;
                dfs(index + 1);
                check[a] = 0;
            }
        }
    }
}
int main() {
    scanf("%d", &k);
    for (int a = 0; a < k; ++a) {
        scanf(" %c", &arr[a]);
    }

    for (int a = 0; a < 10; ++a) {
        ar[0] = a;
        check[a] = 1;
        dfs(0);
        check[a] = 0;
    }
    

    return 0;
}
