#include <cstdio>
using namespace std;

int dp[100000 + 1][3] = { 0, };
int main() {
    int n;
    scanf("%d",&n);
    dp[1][0] = 1;
    dp[2][1] = 1;
    dp[3][0] = 1;
    dp[3][1] = 1;
    dp[3][2] = 1;


    for (int a = 4; a <= 100000; ++a) {
        dp[a][0] = (dp[a - 1][1] + dp[a - 1][2])%1000000009;
        dp[a][1] = (dp[a - 2][0] + dp[a - 2][2]) % 1000000009;
        dp[a][2] = (dp[a - 3][0] + dp[a - 3][1]) % 1000000009;
    }
    int num;
    while (n--) {
        scanf("%d", &num);
        printf("%d\n", ((dp[num][0] + dp[num][1]) % 1000000009 + dp[num][2]) % 1000000009));
    }
    return 0;
}
