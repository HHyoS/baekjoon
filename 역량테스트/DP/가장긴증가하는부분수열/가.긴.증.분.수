#include <bits/stdc++.h>
using namespace std;

int main() {
	ios::sync_with_stdio(false);
	cin.tie(NULL);
    int n;
    cin >> n;
    vector<int> A(n);
    for (int i = 0; i < n; ++i) {
        cin >> A[i];
    }
    vector<int> dp(n, 1);
    dp[0] = 1;
    for (int j = 1; j < n; ++j) {
        for (int i = 0; i < j; ++i) {
            if (A[i] < A[j]) {
                dp[j] = max(dp[j], dp[i] + 1);
            }
        }
    }
    cout << *max_element(dp.begin(), dp.end()) << "\n";
}
